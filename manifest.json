{
  "manifest_version": 3,
  "name": "Near horizon",
  "version": "0.0.1",
  "action":{
      "default_popup": "index.html",
      "default_title": "Near horizon"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://near.org/*"],
      "js": ["script.js"]
    }
  ]
}
